{
  "name": "@palladxyz/ui",
  "version": "0.1.0",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "prepare": "panda codegen",
    "build": "tsup",
    "build:css": "panda cssgen --outfile dist/styles.css",
    "dev": "tsup --watch",
    "test:unit": "echo 'no tests yet'",
    "test:e2e": "playwright test",
    "story:dev": "ladle serve",
    "story:build": "ladle build -o ./build",
    "story:preview": "ladle preview",
    "cleanup": "rimraf node_modules dist .turbo"
  },
  "dependencies": {
    "@github/hotkey": "^2.0.1",
    "@johnn-e/react-mesh-gradient": "^1.1.0",
    "@ladle/react": "^2.13.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "expo-modules-core": "^1.2.7",
    "lucide-react": "^0.255.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "universal-tooltip": "^0.5.0",
    "zeego": "^1.6.2"
  },
  "devDependencies": {
    "@hyrious/esbuild-plugin-commonjs": "^0.2.2",
    "@pandacss/dev": "^0.6.0",
    "@types/react": "^18.0.28",
    "esbuild-plugin-alias": "^0.2.1",
    "vite": "^4.3.9"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
